<div class="w3-section">
    <div class="w3-container">
        <div class="w3-black w3-card-2 w3-content" style="height: 100px; width: 100px">&nbsp;</div>
        <div class="w3-padding-24 w3-center">
            <div class="w3-larger"><b><%= data.first_name+" "+data.middle_name+" "+data.last_name %></b></div>
            <div>Reg. No.: <%= data.reg_number %></div>
            <hr />
        </div>
        <div class="w3-content" style="max-width: 700px">
            <div class="w3-row-padding w3-margin-top">
                <div class="w3-col l4 m5 s6 w3-padding">First Name</div>
                <div class="w3-col l8 m7 s6 w3-padding w3-light-grey"><%= data.first_name %></div>
            </div>

            <div class="w3-row-padding w3-margin-top">
                <div class="w3-col l4 m5 s6 w3-padding">Middle Name</div>
                <div class="w3-col l8 m7 s6 w3-padding w3-light-grey"><%= data.middle_name %></div>
            </div>

            <div class="w3-row-padding w3-margin-top">
                <div class="w3-col l4 m5 s6 w3-padding">Last Name</div>
                <div class="w3-col l8 m7 s6 w3-padding w3-light-grey"><%= data.last_name %></div>
            </div>

            <div class="w3-row-padding w3-margin-top">
                <div class="w3-col l4 m5 s6 w3-padding">Age (Years)</div>
                <div class="w3-col l8 m7 s6 w3-padding w3-light-grey"><%= data.age %></div>
            </div>

            <div class="w3-row-padding w3-margin-top">
                <div class="w3-col l4 m5 s6 w3-padding">Level (Class)</div>
                <div class="w3-col l8 m7 s6 w3-padding w3-light-grey"><%= data.level %></div>
            </div>

            <div class="w3-row-padding w3-margin-top">
                <div class="w3-col l4 m5 s6 w3-padding">Department</div>
                <div class="w3-col l8 m7 s6 w3-padding w3-light-grey"><%= data.department %></div>
            </div>
        </div>
        <div class="w3-padding-24 w3-center">
            <hr />
            <a href="/list" class="w3-btn w3-blue w3-hover-blue w3-margin-left"><i class="fa fa-list"></i> List Students</a>

            <a href="<%= "/update/"+ data.reg_number %>" class="w3-btn w3-blue w3-hover-blue w3-margin-left"><i class="fa fa-edit"></i> Update</a>
        </div>
    </div>

</div>
