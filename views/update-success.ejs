<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title><%= title %></title>
        <% include partials/core_styles %>
    </head>
    <body>
        <% include partials/navigation %>
        <div class="w3-container w3-margin-top w3-margin-bottom">
            <div class="w3-content w3-white w3-padding-32" style="max-width: 900px">
                <div class="w3-padding w3-text-green w3-large w3-center"><b>PROFILE UPDATED SUCESSFULLY</b></div>
                <% if (data) { %>
                    <% include partials/profile %>
                <% } else { %>
                    <div class="w3-padding w3-center w3-border-top">
                        <a href="<%= "/view/" + reg_number %>" class="w3-btn w3-blue w3-hover-blue w3-margin-right">View Profile</a>
                        <a href="/list" class="w3-btn w3-blue w3-hover-blue w3-margin-left">List Students</a>
                    </div>
                <% } %>
            </div>
        </div>
        <% include partials/core_scripts %>
    </body>
</html>
